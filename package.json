    "deploy": "yarn build && vercel --prod",
    "vercel-build": "cd ../frontend && yarn && yarn build",
    "cy:test": "npx cypress open"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "connect-mongodb-session": "^5.0.0",
    "cypress": "^15.4.0",
    "dotenv": "^17.2.3",
    "graphql-upload": "^17.0.0"
  }
}
